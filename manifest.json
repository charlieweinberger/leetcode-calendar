{
  "version": "1.1.0",
  "manifest_version": 3,
  "name": "Leetcode Calendar Extension",
  "description": "View your leetcode progress in a calendar when you open a new tab!",
  "action": {
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://leetcode.com/graphql",
    "https://api.resend.com/emails"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon-32.png",
        "icon-128.png"
      ],
      "matches": []
    }
  ]
}
